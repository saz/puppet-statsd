, cloudwatch: {
    accessKeyId: '<%= access_key_id %>'
    , secretAccessKey: '<%= secret_access_key %>'
    , region: '<%= region %>'
<% if @namespace -%>
    , namespace: '<%= namespace %>'
<% end -%>
<% if @metric_name -%>
    , metricName: '<%= metric_name %>'
<% end -%>
    , processKeyForNames: <%= process_key_for_names %>
  }
